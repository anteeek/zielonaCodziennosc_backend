{
  "version": 2,
  "name": "hackheroes_server",
  "builds": [
    { "src": "index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "JWT_SECRET": "@jwt_secret",
    "HERE_APP_ID": "@here_app_id",
    "HERE_APP_SECRET": "@here_app_secret"
  }
}
